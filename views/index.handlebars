  <title>CCutz Designer Apparel</title>

  <!-- Bootstrap core CSS -->
  <link href="styles/lib/reset.css" rel="stylesheet">
  <link href="styles/lib/bootstrap.min.css" rel="stylesheet">
  <link href="styles/lib/seaweed-script-font.css" rel="stylesheet">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU"
    crossorigin="anonymous">


  <!-- Custom styles for this template -->
  <link href="styles/styles.css" rel="stylesheet">
</head>

<body>

  <header>

    <div class="navbar navbar-dark bg-light shadow-sm">
      <div class="container-fluid d-flex justify-content-between">
        <a href="#" class="navbar-brand d-flex align-items-center">
          <img src="images/logo/cz-seaweed.png" width="50" height="50">
          <h1 class="cz-h1">Designer Apparel</h1>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarHeader" aria-controls="navbarHeader"
          aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
      </div>
    </div>
    <div class="collapse bg-light" id="navbarHeader">
      <div class="container">
        <div class="row">
          <div class="col-sm-8 col-md-7 py-4">
            <h4 class="text-coral">About</h4>
            <p class="text-muted"></p>
          </div>
          <div class="col-sm-4 offset-md-1 py-4">
            <div class="social text-center">
              <a href="https://www.facebook.com/CCutzlabel-927024624142008" target="blank">
                <i class="fab fa-facebook-square"></i>
              </a>
              <a href="https://www.instagram.com/ccutzlabel/" target="blank">
                <i class="fab fa-instagram"></i>
              </a>
              <a href="mailto:marybellapu@gmail.com">
                <i class="fas fa-envelope-square"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>
  </header>

  <main role="main">

    <section class="jumbotron jumbotron-fluid bg-light">
      <div class="container-fluid text-center">

        <video autoplay loop muted class="home-video" width="450" height="300">
          <source src="images/latest1.mp4" type="video/mp4">
          <source src="images/latest1.mp4" type="video/mp4"> Your browser does not support the video tag.
        </video>
        <video autoplay loop muted class="home-video" width="450" height="300">
          <source src="images/latest1.mp4" type="video/mp4">
          <source src="images/latest1.mp4" type="video/mp4"> Your browser does not support the video tag.
        </video>
        <video autoplay loop muted class="home-video" width="450" height="300">
          <source src="images/latest1.mp4" type="video/mp4">
          <source src="images/latest1.mp4" type="video/mp4"> Your browser does not support the video tag.
        </video>

      </div>
    </section>

    <section class="container text-center">
      <h1 class="cz-h1">Collection</h1>
       <form class="form-inline mt-2 mt-md-0 float-right">
            <input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit"><i class="fa fa-search"></i></button>
          </form>
      <div class="navbar navbar-light bg-light shadow-sm">
        <div class="container d-flex justify-content-between">
          <a href="#" class=" d-flex align-items-center">All</a>
          <a href="#" class=" d-flex align-items-center">Latest</a>
          <a href="#" class=" d-flex align-items-center">Spring</a>
          <a href="#" class=" d-flex align-items-center">Summer</a>
          <a href="#" class=" d-flex align-items-center">Wedding</a> 
        </div>
      </div>


      <div class="row">

        <div class="col-md-4">
          <div class="card mb-4 shadow-sm">
            <img class="card-img-top" data-src="holder.js/100px225?theme=thumb&bg=55595c&fg=eceeef&text=Thumbnail" alt="Card image cap">
            <div class="card-body">
              <p class="card-text">{{name}}</p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                  <button type="button" class="btn btn-sm btn-outline-secondary">View</button>
                </div>
                <small class="text-muted">{{type}}</small>
                <small class="text-muted">{{material}}</small>
                <small class="text-muted">{{colors}}</small>
                <small class="text-muted">{{price}}</small>
              </div>
            </div>
          </div>
        </div>
    </section>

  </main>

  <footer class="text-muted">
    <div class="container">
      <p class="float-left text-coral">&copy; CCutz</p>

      <p class="float-right">
        <a href="#">Back to top</a>
      </p>
      <div class="social text-center">
        <a href="https://www.facebook.com/CCutzlabel-927024624142008" target="blank">
          <i class="fab fa-facebook-square"></i>
        </a>
        <a href="https://www.instagram.com/ccutzlabel/" target="blank">
          <i class="fab fa-instagram"></i>
        </a>
        <a href="mailto:marybellapu@gmail.com">
          <i class="fas fa-envelope-square"></i>
        </a>
      </div>
    </div>
  </footer>
